RewriteEngine On # Activer le module Rewrite
RewriteRule ^users/?$ users.php [NC,L]
RewriteRule ^[^/]+/(\d+)$ users.php?id=$1

RewriteRule ^commandes/?$ commandes.php [NC,L]
RewriteRule ^[^/]+/(\d+)$ commandes.php?id=$1


RewriteRule ^commentaire/?$ commentaire.php [NC,L]
RewriteRule ^[^/]+/(\d+)$ commentaire.php?id=$1